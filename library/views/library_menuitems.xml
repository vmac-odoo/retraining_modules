<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="book_list_action" model="ir.actions.act_window">
        <field name="name">Book List</field>
        <field name="res_model">library.book</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create the first book
            </p>
        </field>
    </record>
    <menuitem
            name="Library"
            id="menu_library"
            sequence="1"
            web_icon="library,static/description/icon.png"
    >
        <menuitem id="menu_library_sub" name="Library" sequence="1">
            <menuitem id="menu_library_books" name="Books" sequence="2" action="library.book_list_action"/>
        </menuitem>
    </menuitem>
</odoo>
